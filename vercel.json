{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "https://videotube-backend-delta.vercel.app/api/$1"
      }
    ],
    "env": {
      "VITE_API_BASE_URL": "https://videotube-backend-delta.vercel.app/api/v1"
    }
  }
  